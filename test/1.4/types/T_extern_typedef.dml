dml 1.4;

device test;

header %{ typedef struct { int _; struct { int _; int x; } y; } z_t; %}
extern    typedef struct {        struct {        int x; } y; } z_t;

session z_t c = {{7}};

method init() {
    local z_t a;
    local z_t b = {{5}};
    a.y.x = b.y.x;
    assert a.y.x == 5;
    a.y = c.y;
    assert a.y.x == 7;

    // This tests that GCC considers uint64_t and uintptr_t compatible
    local uintptr_t *p = NULL;
    local uint64_t *pp = p;
}
